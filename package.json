{
  "name": "understrap-dfrost",
  "version": "0.1.1",
  "description": "DFrost Child Theme for UnderStrap Theme Framework: https://gitlab.vi-arise.com/dfrost/dfrost-homepage.git",
  "main": "index.js",
  "scripts": {
    "css:front": "node_modules/sass/sass.js --watch assets/sass/child-theme.scss:assets/css/child-theme.min.css",
    "css:admin": "node_modules/sass/sass.js --watch assets/sass/admin.scss:assets/css/admin.min.css",
    "js:dev": "webpack --progress --mode=development",
    "all:dev": "npm run js:dev && node_modules/sass/sass.js assets/sass/child-theme.scss:assets/css/child-theme.min.css"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.vi-arise.com/dfrost/dfrost-homepage.git"
  },
  "keywords": [
    "wordpress",
    "theme",
    "framework",
    "bootstrap",
    "underscores"
  ],
  "author": "VI ARISE",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://gitlab.vi-arise.com/dfrost/dfrost-homepage.git"
  },
  "homepage": "https://understrap.com",
  "dependencies": {
    "animate.css": "^3.7.2",
    "bootstrap": "^4.4.1",
    "bootstrap-breakpoints": "^1.0.0",
    "gsap": "^3.2.6",
    "gulp": "^4.0.2",
    "jquery-visible": "^1.2.0",
    "owl.carousel": "^2.3.4",
    "popper.js": "^1.14.3",
    "sass": "^1.26.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "7",
    "babel-preset-env": "^1.4.0",
    "css-loader": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "mixitup": "^3.3.1",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "remove-strict-webpack-plugin": "^0.1.2",
    "sass-loader": "^7.3.1",
    "script-loader": "^0.7.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7"
  }
}
